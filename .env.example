# Sample .env file for docker-compose or docker-manage.sh
# Copy this file to .env and edit the values as needed

# PIA Credentials
PIA_USER=your_pia_username
PIA_PASS=your_pia_password

# Comma-separated list of regions
REGIONS=uk,france,nl_amsterdam,de-frankfurt,swiss

# Cron schedule expression (every 12 hours by default)
UPDATE_INTERVAL=0 */12 * * *

# Enable debug output (0=disabled, 1=enabled)
DEBUG=0
